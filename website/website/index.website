<script>
    $(document).ready(function(){
        // Smooth scrolling for links with hash tags
        $('a[href^="#"]').on('click', function(event) {
            var target = $(this.getAttribute('href'));
            if( target.length ) {
                event.preventDefault();
                $('html, body').stop().animate({
                    scrollTop: target.offset().top
                }, 1000);
           }
    });
 });
</script>
